extends commonLayout

block content

  div(data-role="controlgroup", data-theme='b')
    select(id='select-slide-edit')
      -for (var i=0; i < slides.length; i++)
        -var slide = slides[i];
        option(value="#{slide._id}", label="#{slide.name}")=slide.name
  div(id='photos-container')

  div(data-role="controlgroup", data-theme='b')
    a(id="slide-edit-btn", class='ui-btn ui-btn-icon-left ui-icon- input_60P') Delete

  script(src='//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js')
  script(src="/javascripts/piframe.js")